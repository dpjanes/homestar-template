--
--  compile to JSON with 'iotql-model Template.iotql'
--
CREATE MODEL Template WITH
    schema:name = "Template",
    schema:description = "Template"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean,
    iot:write = true,
    iot:read = true,
    iot:actuator = true,
    iot:sensor = true
;
